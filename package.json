{
  "name": "money-tracker",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/weizhongtan/money-tracker"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen",
    "db": "pgcli postgres://postgres:postgrespassword@localhost:5432/moneytracker",
    "api:console": "hasura console --project hasura",
    "api:up": "DB_NAME=moneytracker DB_PASSWORD=postgrespassword docker-compose up -d --force-recreate",
    "api:down": "docker-compose down",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.5",
    "@ant-design/icons": "^4.2.2",
    "@apollo/client": "^3.1.3",
    "@babel/runtime": "^7.11.2",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@nivo/bar": "^0.61.1",
    "@nivo/line": "^0.61.1",
    "@nivo/pie": "^0.61.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-timeago": "^4.1.1",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^8.3.0",
    "antd": "^4.10.0",
    "csvjson": "^5.1.0",
    "dayjs": "^1.10.3",
    "graphql": "^14.7.0",
    "hasura-cli": "^1.3.3",
    "ofx-js": "^0.1.1",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-debounce-input": "^3.2.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-timeago": "^4.4.0",
    "react-virtualized-auto-sizer": "^1.0.4",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7",
    "uuid": "^7.0.3",
    "with-url-state": "^3.0.0-beta.3"
  },
  "proxy": "http://localhost:8080/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "devDependencies": {}
}
