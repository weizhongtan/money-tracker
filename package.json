    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "morgan": "^1.10.0",
    "ofx-js": "^0.1.1",
    "react": "^16.13.1",
    "react-debounce-input": "^3.2.2",
