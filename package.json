{
  "name": "money-tracker",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weizhongtan/money-tracker.git"
  },
  "author": "weizhongtan <me@wztan.com>",
  "homepage": "https://github.com/weizhongtan/money-tracker#readme",
  "scripts": {
    "client:dev": "vite serve packages/client",
    "client:build": "tsc --project packages/client/tsconfig.json && vite build packages/client",
    "client:serve": "vite preview packages/client",
    "api:dev": "nodemon --config packages/api/nodemon.json",
    "api:up": "DB_NAME=moneytracker DB_PASSWORD=postgrespassword docker-compose -f packages/api/docker-compose.yaml up -d --force-recreate",
    "api:down": "docker-compose -f packages/api/docker-compose.yaml down",
    "api:console": "hasura console --project packages/api/hasura",
    "db": "pgcli postgres://postgres:postgrespassword@localhost:5432/moneytracker",
    "generate": "graphql-codegen",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
